<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.elmo.robella.mapper.ModelMapper">

    <select id="findByPublishedTrue" resultType="org.elmo.robella.model.entity.Model">
        SELECT * FROM model WHERE published = true ORDER BY id
    </select>

    <select id="findByModelKey" resultType="org.elmo.robella.model.entity.Model">
        SELECT * FROM model WHERE model_key = #{modelKey}
    </select>

</mapper>